{
    "name": "jlorente/laravel-zadarma",
    "description": "Laravel ^5.6 integration for the Jlorente Zadarma package.",
    "keywords": [
        "php",
        "jlorente",
        "laravel",
        "zadarma"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "JosÃ© Lorente",
            "email": "jose.lorente.martin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.5",
        "jlorente/zadarma-php-sdk": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Jlorente\\Laravel\\Zadarma\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jlorente\\Laravel\\Zadarma\\ZadarmaServiceProvider"
            ],
            "aliases": {
                "Zadarma": "Jlorente\\Laravel\\Zadarma\\Facades\\Zadarma"
            }
        }
    },
    "minimum-stability": "stable"
}
